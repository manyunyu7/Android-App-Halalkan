<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <View
        android:id="@+id/separator"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#EEEEEE"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@id/tv_position"
        />

    <ImageView
        android:id="@+id/img_group_member"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_popup_dialog_users"
        android:layout_marginStart="@dimen/dimen_20dp"
        android:layout_marginTop="@dimen/dimen_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/separator"
        />

    <TextView
        android:id="@+id/label_group_member"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="16sp"
        android:textColor="#000000"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toTopOf="@id/img_group_member"
        app:layout_constraintStart_toEndOf="@id/img_group_member"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Group Member"/>

    <TextView
        android:id="@+id/label_display_group_member"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="14sp"
        android:textColor="#767676"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toBottomOf="@id/label_group_member"
        app:layout_constraintStart_toEndOf="@id/img_group_member"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Chief Technology Officer"/>

    <ImageView
        android:id="@+id/img_email"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_popup_dialog_email"
        android:layout_marginStart="@dimen/dimen_20dp"
        android:layout_marginTop="@dimen/dimen_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_display_group_member"
        />

    <TextView
        android:id="@+id/label_email"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="16sp"
        android:textColor="#000000"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toTopOf="@id/img_email"
        app:layout_constraintStart_toEndOf="@id/img_email"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Email"/>

    <TextView
        android:id="@+id/label_display_email"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="14sp"
        android:textColor="#1F8DD6"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toBottomOf="@id/label_email"
        app:layout_constraintStart_toEndOf="@id/img_email"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Chief Technology Officer"/>


    <ImageView
        android:id="@+id/img_phone"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_popup_dialog_phone"
        android:layout_marginStart="@dimen/dimen_20dp"
        android:layout_marginTop="@dimen/dimen_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_display_email"
        />

    <TextView
        android:id="@+id/label_phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="16sp"
        android:textColor="#000000"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toTopOf="@id/img_phone"
        app:layout_constraintStart_toEndOf="@id/img_phone"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Nomor Handphone" />

    <TextView
        android:id="@+id/label_display_phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="14sp"
        android:textColor="#1F8DD6"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toBottomOf="@id/label_phone"
        app:layout_constraintStart_toEndOf="@id/img_phone"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Chief Technology Officer"/>

    <ImageView
        android:id="@+id/img_gender"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/ic_popup_dialog_user"
        android:layout_marginStart="@dimen/dimen_20dp"
        android:layout_marginTop="@dimen/dimen_16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_display_phone"
        />

    <TextView
        android:id="@+id/label_gender"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="16sp"
        android:textColor="#000000"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toTopOf="@id/img_gender"
        app:layout_constraintStart_toEndOf="@id/img_gender"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Jenis Kelamin" />

    <TextView
        android:id="@+id/label_display_gender"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textAlignment="textStart"
        android:textSize="14sp"
        android:textColor="#767676"
        android:fontFamily="@font/poppins_regular"
        android:layout_marginHorizontal="@dimen/dimen_16dp"
        app:layout_constraintTop_toBottomOf="@id/label_gender"
        app:layout_constraintStart_toEndOf="@id/img_gender"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Chief Technology Officer"/>

</merge>